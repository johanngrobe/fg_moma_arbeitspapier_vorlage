  keywords = {ecology,environmental factors,environmental policy,Erfolg,Gro√üstadt,innovation,Innovation,large city,√ñkologie,political change,political governance,politische Steuerung,politischer Wandel,Stadt,success,town,traffic,transportation policy,Umweltfaktoren,Umweltpolitik,Verkehr,Verkehrspolitik},
  file = {C\:\\Users\\johan\\Zotero\\storage\\86BSUJ83\\Bratzel - Umweltorientierter Verkehrspolitikwandel in St√§dte.pdf;C\:\\Users\\johan\\Zotero\\storage\\GVMUBUAY\\12554.html}
