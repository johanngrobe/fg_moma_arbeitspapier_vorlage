@online{liu2023,
  title = {G-{{Eval}}: {{NLG Evaluation}} Using {{GPT-4}} with {{Better Human Alignment}}},
  shorttitle = {G-{{Eval}}},
  author = {Liu, Yang and Iter, Dan and Xu, Yichong and Wang, Shuohang and Xu, Ruochen and Zhu, Chenguang},
